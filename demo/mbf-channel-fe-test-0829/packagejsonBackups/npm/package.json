{
  "name": "@mu/channel-template",
  "version": "1.0.0-alpha.21",
  "description": "Multiple platform front end component",
  "main": "dist/index.js",
  "main:h5": "dist/h5/index.js",
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev:weapp": "madp build --type weapp -- --watch",
    "dev:qq": "madp build --type qq -- --watch",
    "dev:h5": "madp build --type h5 -- --watch debug",
    "dev:swan": "madp build --type swan -- --watch",
    "dev:alipay": "madp build --type alipay -- --watch",
    "build:component": "cross-env TARO_BUILD_TYPE=component madp build --ui",
    "prepublishOnly": "npm run build:component",
    "lint": "eslint ./src --fix",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test": "NODE_ENV=test && jest --coverage",
    "test:ci": "npm run build:h5 && npm run test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "handle-tag": "node ./script/handle-tag.js",
    "release": "standard-version",
    "release:alpha": "npm run release -- -p alpha && git push --follow-tags && npm publish --tag alpha",
    "release:beta": "npm run release -- -p beta && git push --follow-tags && npm publish --tag beta",
    "publish:npm": "npm run release && git push --follow-tags && npm publish",
    "switch": "cross-env PACKAGE=project node config/packagejson-switch.js",
    "autoSwitch": "cross-env PACKAGE=project TYPE=auto node config/packagejson-switch.js"
  },
  "keywords": [
    "taro",
    "h5 miniapp",
    "component"
  ],
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "dependencies": {
    "@mu/basic-library": "1.7.3-beta.3",
