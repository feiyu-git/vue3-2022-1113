{
  "name": "@mu/channel-template",
  "version": "1.1.0-23",
  "description": "Multiple platform front end component",
  "main": "dist/index.js",
  "main:h5": "dist/h5/index.js",
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "templateInfo": {
    "typescript": false,
    "mobx": true
  },
  "scripts": {
    "prebuild:h5": "node config/entry.js",
    "build:h5": "madp config && madp build --type h5",
    "build:weapp": "madp config && madp build --type weapp",
    "build:alipay": "madp config && madp build --type alipay",
    "build:swan": "madp config && taro build --type swan",
    "build:tt": "madp config && taro build --type tt",
    "build:rn": "madp build --type rn",
    "build:quickapp": "madp build --type quickapp",
    "build:qq": "madp build --type qq",
    "build:h5-report": "madp build --type h5 report",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch debug",
    "dev:h5-st1": "cross-env ENV=st1 npm run dev:h5",
    "dev:h5-se1": "cross-env ENV=se1 npm run dev:h5",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "lint": "eslint --ext .js,.jsx src --fix",
    "prd:server": "madp server",
    "build:h5-offline": "cross-env BT=offline npm run build:h5",
    "dev:h5-offline": "cross-env BT=offline npm run dev:h5",
    "dev:h5-sk": "cross-env ENV=st1 BT=sk CHANNEL=0APP npm run dev:h5",
    "index1": "cross-env ENTRY=index npm run dev:h5",
    "switch": "cross-env PACKAGE=npm node config/packagejson-switch.js",
    "build:component": "cross-env TARO_BUILD_TYPE=component madp build --ui",
    "prepublishOnly": "npm run build:component",
    "release": "standard-version",
    "release:alpha": "npm run release -- -p && git push --follow-tags && npm publish --tag alpha",
    "publish:npm": "npm run release && git push --follow-tags && npm publish"
  },
  "author": "mucfc",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js, jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@mu/basic-library": "1.7.3-beta.3",
    "@mu/bio-auth-zfb": "2.1.0-beta.10",
    "@mu/biometrics": "1.4.0-beta.1",
    "@mu/credit-card": "1.7.9-alpha.18",
    "@mu/channel-repay-services": "1.2.1-alpha.17",
    "@mu/coupon-selector": "3.4.3",
    "@mu/loan-help": "1.0.1-beta.13",
    "@mu/short-loan-card": "1.2.5",
    "@mu/lui": "2.6.4-basic.28",
    "@mu/login-popup": "1.5.3-nostyle.3",
    "@mu/madp-utils": "1.6.8",
    "@mu/safe-sms-code": "1.3.5-beta.35",
    "@mu/survey": "1.63.0",
    "@mu/tarosdk-mu-bio-auth": "1.5.0-beta.2",
    "@mu/unhandle-notice": "1.0.6-beta.21",
    "@mu/user-bhvr-detector": "1.1.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@mu/madp-cli": "1.5.19",
    "@mu/geestest": "1.5.3-beta.0",
    "@types/react": "16.9.26",
    "@types/webpack-env": "1.15.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-jsx-stylesheet": "0.6.9",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "3.1.0",
    "image-webpack-loader": "6.0.0",
    "lint-staged": "9.5.0",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "script-ext-html-webpack-plugin": "2.1.5",
    "standard-version": "6.0.1",
    "vconsole-webpack-plugin": "1.5.2",
    "webpack-bundle-analyzer": "3.6.1",
    "@mu/basic-library": "1.7.3-beta.3",
    "build-plugin-component": "1.0.0",
    "build-scripts": "1.1.1",
    "enzyme": "3.10.0",
    "prop-types": "15.5.8",
    "enzyme-adapter-react-16": "1.14.0",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-transform-react-jsx": "7.9.4",
    "@babel/preset-env": "7.9.0",
    "classnames": "2.2.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "conventional-changelog-cli": "2.0.31",
    "mini-css-extract-plugin": "0.9.0",
    "cz-conventional-changelog": "2.1.0",
    "jest": "23.6.0",
    "nerv-server": "1.5.7",
    "nerv-test-utils": "1.5.7",
    "nervjs": "1.5.7",
    "typescript": "3.8.3",
    "vconsole": "3.3.4",
    "webpack": "4.42.1",
    "webpack-merge": "4.2.2",
    "@types/node": "12.12.31",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "commitlint": "8.3.5",
    "eslint-config-o2team": "0.1.6",
    "eslint-plugin-jest": "21.27.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.16.0",
    "fs-extra": "8.1.0"
  }
}
